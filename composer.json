{
  "name": "alexrojer31/logging",
  "description": "Система логирования для приложений",
  "homepage": "https://alexrojer31.ru",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/alexrojer31/messages.git"
    },
    {
      "type": "git",
      "url": "https://github.com/alexrojer31/filesystem.git"
    }
  ],
  "require": {
    "php": ">=7.1.0",
    "alexrojer31/messages": "^1.0.0",
    "alexrojer31/filesystem": "^1.0.0"
  },
  "require-dev": {
    "phpunit/phpunit": "7.5.20"
  },
  "suggest": {
    "alexrojer31/messages": "Различные классы сообщений",
    "alexrojer31/filesystem": "Классы для работы с файловой системой"
  },
  "autoload": {
    "psr-4": {
      "Logging\\": "src/"
    }
  },
  "scripts": {
    "test:all": "./vendor/bin/phpunit --configuration phpunit.xml --testsuite All"
  }
}
